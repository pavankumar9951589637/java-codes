55 89 144 233 377 

import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner s = new Scanner(System.in);
        int a = s.nextInt();
        int b=0;
        int c=1;
        for(int i=1;i<=a;i++){
            for(int j=1;j<=i;j++){
                System.out.print(b+" ");
                int d=b+c;
                b=c;
                c=d;

            }
            System.out.println();

        }


    }
}
