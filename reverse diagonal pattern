			*
		*
	*
*

import java.util.*;
public class Main{
    public static void main(String[] args){
        Scanner s=new Scanner(System.in);
        int a=s.nextInt();
        int sp=4;
        for(int i=0;i<a;i++){
            for(int j=0;j<sp;j++){
                System.out.print("\t");
            }
            System.out.print("*\n");
            sp--;
        }

        }
    }


